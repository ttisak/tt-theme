  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "source.diff"
      ],
      "settings": {
        "foreground": "#8893a5"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.diff.range",
        "punctuation.definition.range.diff"
      ],
      "settings": {
        "foreground": "#5e0b99"
      }
    },
    {
      "scope": [
        "keyword",
        "punctuation.definition.keyword",
        "variable.language",
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted",
        "punctuation.definition.from-file.diff"
      ],
      "settings": {
        "foreground": "#b00210"
      }
    },
    {
      "scope": [
        "constant",
        "support.constant"
      ],
      "settings": {
        "foreground": "#22b1c4"
      }
    },
    {
      "scope": [
        "storage",
        "support.class",
        "entity.name.namespace",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#0060ae"
      }
    },
    {
      "scope": [
        "markup.inline.raw.string",
        "string",
        "markup.inserted",
        "punctuation.definition.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.to-file.diff"
      ],
      "settings": {
        "foreground": "#589929"
      }
    },
    {
      "scope": [
        "entity.name.section",
        "entity.name.tag",
        "support.type"
      ],
      "settings": {
        "foreground": "#9e6700"
      }
    },
    {
      "scope": [
        "support.type.property-name",
        "support.variable",
        "variable"
      ],
      "settings": {
        "foreground": "#242d3d"
      }
    },
    {
      "scope": [
        "entity.other",
        "punctuation.definition.entity",
        "support.other"
      ],
      "settings": {
        "foreground": "#b46113"
      }
    },
    {
      "scope": [
        "meta.brace",
        "punctuation"
      ],
      "settings": {
        "foreground": "#36435d"
      }
    },
    {
      "name": "bold",
      "scope": [
        "markup.bold",
        "punctuation.definition.bold",
        "entity.other.attribute-name.id"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "italic",
      "scope": [
        "comment",
        "markup.italic",
        "punctuation.definition.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity.name.type",
      ],
      "settings": {
        "foreground": "#0e75b6"
      }
    }
  ]
}